function DrawStep(h_stepPlot,stepData,result)
L_value = result(2,:);
R_value = result(3,:);
timestamp = result(1,:);
i=1;
L_target = [];
R_target = [];
while ~strcmp(stepData{i,3},'')
    L_target = [L_target; zeros(str2num(stepData{i,3}),1) + str2num(stepData{i,1})];
    R_target = [R_target; zeros(str2num(stepData{i,3}),1)+str2num(stepData{i,2})];
    i=i+1;
end

